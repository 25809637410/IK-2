to Sans KR', sans-serif;\n  /*  font-family: 'Poppins', sans-serif; */\n}\n\nbody {\n  width: 100%;\n  height: 100%;\n  background-color: #f8f8f8;\n  overflow-x: hidden;\n  overflow-y: hidden;\n}\n\nli {\n  list-style-type: none;\n}\n\na {\n  text-decoration: none;\n  color: #222;\n}\n\n$color : #FD8234;\n\n@mixin size($width, $height) {\n  width: $width;\n  height: $height;\n}\n\n.container {\n  width: 100%;\n  height: auto;\n  margin-top: 80px;\n\n\n  .contents_wrap {\n    position: relative;\n    @include size(100%, calc(100vh - 80px));\n    margin: 0 auto;\n    display: flex;\n\n    .left_section {\n      @include size(820px, auto);\n\n      .detail_swiper {\n        .swiper-pagination {\n          .swiper-pagination-bullet {\n            @include size(10px, 10px);\n          }\n\n          .swiper-pagination-bullet-active {\n            background-color: #fff;\n          }\n\n          .swiper-pagination-bullet:hover {\n            transform: scale(1.1);\n            transition: all 0.3s;\n          }\n        }\n      }\n\n      .content_tab {\n        position: relative;\n        height: 85px;\n\n        .tabmenu_list {\n          position: absolute;\n          @include size(820px, 85px);\n          display: flex;\n          align-items: center;\n          text-align: center;\n          background-color: #fff;\n\n          .tabmenu {\n            height: 100%;\n            line-height: 85px;\n          }\n\n          .bar {\n            position: absolute;\n            bottom: 0;\n            left: 0;\n            height: 5px;\n            background: $color;\n          }\n        }\n      }\n    }\n  }\n}"},"lineCount":null}},"error":null,"hash":"99a43be0af71fdd3ee4455c4627858ad","cacheData":{"env":{}}}